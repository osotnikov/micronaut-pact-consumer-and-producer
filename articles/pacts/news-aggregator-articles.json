{
  "provider": {
    "name": "articles"
  },
  "consumer": {
    "name": "news-aggregator"
  },
  "interactions": [
    {
      "description": "test get article with key\u003dlatest",
      "request": {
        "method": "GET",
        "path": "/articles",
        "query": {
          "key": [
            "latest"
          ]
        },
        "matchingRules": {
          "query": {
            "key": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "latest"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "text/plain; charset\u003dISO-8859-1"
        },
        "body": "Perseverance Landing - February 18, 2021"
      },
      "providerStates": [
        {
          "name": "article exists for key\u003dlatest"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.6.10"
    }
  }
}